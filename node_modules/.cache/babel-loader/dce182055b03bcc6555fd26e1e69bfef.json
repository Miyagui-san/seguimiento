AAA","sourcesContent":["<template>\r\n    <v-div class=\"text-center\" flat>\r\n        <v-dialog v-model=\"dialog\" max-width=\"600px\">\r\n                <template v-slot:activator=\"{ on }\">\r\n                    <v-btn flat class=\"success lighten-4 black--text\" v-on=\"on\">\r\n                        <span>CREAR USUARIO</span>\r\n                    </v-btn>\r\n                </template>\r\n                    <v-card> \r\n                        <v-card-title>\r\n                            <h2>NUEVO USUARIO</h2>\r\n                        </v-card-title>\r\n                        <v-card-text>\r\n                            <v-form class=\"px-3\" ref=\"form\">\r\n                                <v-text-field label=\"Nombre\" v-model=\"nombre\" prepend-icon=\"mdi-folder\" :rules=\"inputRules\"></v-text-field>\r\n                                <v-text-field label=\"Apellido\" v-model=\"apellido\" prepend-icon=\"mdi-pencil\" :rules=\"inputRules\"></v-text-field>\r\n                                <v-text-field label=\"E-mail\" v-model=\"mail\" prepend-icon=\"mdi-pencil\" :rules=\"inputRules\"></v-text-field>\r\n\r\n                                <v-btn flat class=\"success mx-0 mt-3\" @click=\"submit\" :loading=\"loading\">Add project</v-btn>\r\n                            </v-form>\r\n                        </v-card-text>\r\n\r\n                    </v-card>\r\n        </v-dialog>\r\n    </v-div>\r\n</template>\r\n\r\n<script>\r\n  import moment from 'moment';\r\n  import db from '@/fb';\r\n  export default {\r\n    data () {\r\n      return {\r\n        dialog: false,\r\n        nombre: '',\r\n        apellido:'',\r\n        mail:'',\r\n        inputRules: [\r\n            v => v.length >= 3 || 'Minimun length is 3 caracters'\r\n        ],\r\n        loading: false,\r\n      }\r\n    },\r\n    methods: {\r\n        submit() {\r\n            if(this.$refs.form.validate()){\r\n                this.loading = true;\r\n                const usuario = {\r\n                    nombre: this.nombre,\r\n                    apellido: this.apellido,\r\n                    mail: this.mail,\r\n                    person: 'The Net Ninja',\r\n                    status: 'ongoing'\r\n                }\r\n                db.collection('usuarios').add(usuario).then(() => {\r\n                    this.loading = false;\r\n                    this.dialog = false;\r\n                    this.$emit('projectAdded')\r\n                })\r\n            }\r\n        }\r\n    },\r\n    computed: {\r\n        formattedDate() {\r\n            return this.due ? moment(this.due).format(\"Do MMM YYYY\") : ''\r\n        }\r\n    }\r\n  }\r\n</script>"],"sourceRoot":"src/components"}]}